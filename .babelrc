{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "fileName": false,
        "displayName": true
      }
    ],
    ["inline-react-svg"],
    [
      "module-resolver",
      {
        "alias": {
          "@root": "./",
          "@components": "./components"
        }
      }
    ],
    ["inline-dotenv"]
  ]
}
